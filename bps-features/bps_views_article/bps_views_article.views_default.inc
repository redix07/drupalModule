    t('- All -'),
    t('Offset'),
    t('« first'),
    t('‹ previous'),
    t('next ›'),
    t('last »'),
    t('Article - Page - all'),
    t('Article - Block - arch'),
    t('All'),
    t('Article - Page - arch'),
