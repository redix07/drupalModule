  /* Display: Article - Page - all */
  $handler = $view->new_display('page', 'Article - Page - all', 'page');
  $handler->display->display_options['path'] = 'article';

  /* Display: Article - Block - arch */
  $handler = $view->new_display('block', 'Article - Block - arch', 'block_1');
